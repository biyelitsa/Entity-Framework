@using PersonelDbFirst.Models.ViewModel

@model UnvanModel

@{ Layout = "~/Views/Shared/_LayoutPage1.cshtml"; }



<h2>@Model.Baslik</h2>


<div class="row">
    <div class="col-md-5">
        <form method="post">

            <input type="@Model.Type" class="form-control" name="UnvanId" value="@Model.Unvan.UnvanId" />
            <input type="text" class="form-control" name="UnvanAd" value="@Model.Unvan.UnvanAd" />
            <input type="submit" class="@Model.BtnClass" value="@Model.BtnVal" />
            <input type="submit" class="btn btn-warning" value="İptal" formaction="/Unvan/List" />


        </form>
        
    </div>
</div>

